{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "eslint-config-airbnb",
  "plugins": ["react"],
  "rules": {
    "jsx-a11y/href-no-hash": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "max-len": 0,
    "global-require": 0,
    "semi": [2, "never"],
    "comma-dangle": [2, "always-multiline"],
    "no-console": 0
  }
 }
